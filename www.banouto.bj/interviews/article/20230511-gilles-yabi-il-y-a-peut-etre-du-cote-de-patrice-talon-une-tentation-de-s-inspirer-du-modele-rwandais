    <div class="modal-app modal" id="modal-cookie" tabindex="-1" aria-labelledby="AppDownload" aria-hidden="true">
    <div class="modal-app__dialog modal-dialog">
        <div class="modal-app__content modal-content">
            <div class="modal-app__body modal-body">
                <div class="modal-app__logo">
                    <img
                        class="img img-fluid lazyload"
                        src="data:image/svg+xml,%3Csvg xmlns=&#039;http://www.w3.org/2000/svg&#039; viewBox=&#039;0 0 65 65&#039;%3E%3C/svg%3Ehttps://www.banouto.bj/logo-bnt.png"
                        data-src="../../icon.png"
                        alt="Banouto"
                        width="65"
                        height="65"
                    >
                </div>
                <h2 class="modal-app__title">
                    Ce site web utilise des cookies.
                </h2>
                <div style="display: flex; justify-content: center">
                    <p class="modal-app__paragraph laptop" style="max-width: 700px">
                        Les cookies nous permettent de personnaliser le contenu, d'offrir des
                        fonctionnalités relatives aux médias sociaux et d'analyser notre trafic.
                        Ce sont des cookies nécessaires au bon fonctionnement de notre site.
                    </p>
                    <p class="modal-app__paragraph tablet-mobile">
                        Les cookies nous permettent de personnaliser le contenu, d'offrir des
                        fonctionnalités relatives aux médias sociaux et d'analyser notre trafic.
                        Ce sont des cookies nécessaires au bon fonctionnement de notre site.
                    </p>
                </div>
                <button class="btn btn--rounded btn--accent js-cookie-consent-agree" id="closeCookie" onclick="closeCookieModal()">
                    Accepter
                </button>
            </div>
        </div>
    </div>
</div>

    <script>
        window.onload = function() {
            $('#modal-cookie').modal({
                show: true,
                backdrop: 'static',
                keyboard: false
            });
        };

        function closeCookieModal(){
            $('#modal-cookie').modal('hide');
        }

        window.laravelCookieConsent = (function () {

            const COOKIE_VALUE = 1;
            const COOKIE_DOMAIN = 'www.banouto.bj';

            function consentWithCookies() {
                setCookie('banouto_cookie_consent', COOKIE_VALUE, 7300);
                hideCookieDialog();
            }

            function cookieExists(name) {
                return (document.cookie.split('; ').indexOf(name + '=' + COOKIE_VALUE) !== -1);
            }

            function hideCookieDialog() {
                const dialogs = document.getElementsByClassName('js-cookie-consent');

                for (let i = 0; i < dialogs.length; ++i) {
                    dialogs[i].style.display = 'none';
                }
            }

            function setCookie(name, value, expirationInDays) {
                const date = new Date();
                date.setTime(date.getTime() + (expirationInDays * 24 * 60 * 60 * 1000));
                document.cookie = name + '=' + value
                    + ';expires=' + date.toUTCString()
                    + ';domain=' + COOKIE_DOMAIN
                    + ';path=/'
                    + ';samesite=lax';
            }

            if (cookieExists('banouto_cookie_consent')) {
                hideCookieDialog();
            }

            const buttons = document.getElementsByClassName('js-cookie-consent-agree');

            for (let i = 0; i < buttons.length; ++i) {
                buttons[i].addEventListener('click', consentWithCookies);
            }

            return {
                consentWithCookies: consentWithCookies,
                hideCookieDialog: hideCookieDialog
            };
        })();
    </script>

    <!-- /end Cookie -->
